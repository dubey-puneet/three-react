s.state.mylistings;\r\n    let searchlistings = [];\r\n\r\n    for (let i = 0; i < listings.length; i++) {\r\n      if (param === \"All\") {\r\n        searchlistings.push(listings[i]);\r\n      }\r\n      if (\r\n        param === \"Applied\" &&\r\n        listings[i].applied &&\r\n        listings[i].applied.length > 0 &&\r\n        listings[i].status === undefined\r\n      ) {\r\n        searchlistings.push(listings[i]);\r\n      }\r\n      if (\r\n        param === \"Active\" &&\r\n        listings[i].status === \"active\" &&\r\n        (listings[i].complete === undefined ||\r\n          listings[i].confirm === undefined)\r\n      ) {\r\n        searchlistings.push(listings[i]);\r\n      }\r\n      if (\r\n        param === \"Finished\" &&\r\n        listings[i].complete === \"done\" &&\r\n        listings[i].confirm === \"done\"\r\n      ) {\r\n        searchlistings.push(listings[i]);\r\n      }\r\n      if (param === \"Declined\" && listings[i].status === \"declined\") {\r\n        searchlistings.push(listings[i]);\r\n      }\r\n    }\r\n\r\n    this.setState({\r\n      searchlistings: searchlistings,\r\n      showlistshow: !this.state.showlistshow,\r\n      showlist: param,\r\n    });\r\n  };\r\n  sortDate = () => {\r\n    this.state.mylistings.sort(function (a, b) {\r\n      return a.date_created - b.date_created;\r\n    });\r\n  };\r\n  handleModal = () => {\r\n    this.setState({ modalshow: !this.state.modalshow });\r\n  };\r\n  handleModal1 = () => {\r\n    this.setState({ modalshow1: !this.state.modalshow1 });\r\n  };\r\n  componentDidMount() {\r\n    fetchListings().then((listing) => {\r\n      let listings = [];\r\n      for (let index = 0; index < listing.length; index++) {\r\n        let value = listing[index];\r\n        let flag = false;\r\n        if (value.applied === undefined) {\r\n          this.setState({ mylistings: [] });\r\n          continue;\r\n        }\r\n        for (let i = 0; i < value.applied.length; i++) {\r\n          if (value.