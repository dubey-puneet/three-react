tagName === \"LI\") {\r\n      event.target.classList.add(\"active\");\r\n    } else {\r\n      event.target.closest(\"li\").classList.add(\"active\");\r\n    }\r\n  };\r\n  searchlist = (event) => {\r\n    let search = event.target.value;\r\n    let listings = this.state.listings;\r\n    let searchlist = [];\r\n    for (let i = 0; i < listings.length; i++) {\r\n      if (search !== \"\") {\r\n        if (\r\n          listings[i].listing_title\r\n            .toLowerCase()\r\n            .indexOf(search.trim().toLowerCase()) !== -1\r\n        ) {\r\n          searchlist.push(listings[i]);\r\n        }\r\n      } else {\r\n        searchlist.push(listings[i]);\r\n      }\r\n    }\r\n    this.setState({ searchlistings: searchlist });\r\n  };\r\n\r\n  componentDidMount() {\r\n    const pathURL = window.location.pathname;\r\n    fetchUser(pathURL.split(\"/\")[2]).then((user) => {\r\n      this.setState({ userData: user }, () => {\r\n        console.log(this.state.userData);\r\n      });\r\n    });\r\n    channel.on(\"value\", (snap) => {\r\n      let active_listings = 0;\r\n      let jobs_completed = 0;\r\n      let welly_dones = 0;\r\n      let listings = [];\r\n      let reviews = [];\r\n\r\n      let value = snap.val();\r\n      if (value) {\r\n        for (let key in value) {\r\n          if (\r\n            value[key].contact[1] === pathURL.split(\"/\")[2] &&\r\n            value[key].status &&\r\n            value[key].status === \"active\"\r\n          ) {\r\n            active_listings++;\r\n            getListing(value[key].listingID).then((listing) => {\r\n              listings.push({\r\n                id: value[key].listingID,\r\n                listing_title: listing[0].listing_title,\r\n                poster_name: listing[0].firstname + \" \" + listing[0].lastname,\r\n                poster_avatar: listing[0].profileimg\r\n                  ? listing[0].profileimg\r\n                  : defaultAvatar,\r\n              });\r\n              this.setState(